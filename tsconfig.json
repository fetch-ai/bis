{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "sourceMap": true,
    "outDir": "./dist",
    "removeComments": true,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noEmit": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "typeRoots": ["/types", "/node_modules/@overwolf/types"],
  },
  "include": ["src"]
}
